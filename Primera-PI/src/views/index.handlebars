<div>
    <h1>Productos agregados</h1>
    {{#each products}}
        <div>
            <h2>{{this.title}}</h2>
            <div>
                <p>{{this.description}}</p>
                <p>Precio: ${{this.price}}</p>
                <p>Stock: {{this.stock}}</p>
            </div>
        </div>
    {{/each}}
</div>